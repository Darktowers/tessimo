@font-face
    font-family: 'amsi_probold';
    src: url('../fonts/stawix_-_amsipro-bold-webfont.woff2') format('woff2'),
         url('../fonts/stawix_-_amsipro-bold-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
@font-face
    font-family: 'amsi_proregular';
    src: url('../fonts/stawix_-_amsipro-regular-webfont.woff2') format('woff2'),
         url('../fonts/stawix_-_amsipro-regular-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
@font-face 
    font-family: 'grota_sans_rdbold';
    src: url('../fonts/latinotype_-_grotasansrd-bold-webfont.woff2') format('woff2'),
         url('../fonts/latinotype_-_grotasansrd-bold-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
@font-face 
    font-family: 'grota_sans_rdbook';
    src: url('../fonts/latinotype_-_grotasansrd-book-webfont.woff2') format('woff2'),
         url('../fonts/latinotype_-_grotasansrd-book-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
@font-face
    font-family: 'grota_sans_rdlight';
    src: url('../fonts/latinotype_-_grotasansrd-light-webfont.woff2') format('woff2'),
         url('../fonts/latinotype_-_grotasansrd-light-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;
@import "nib"
@import "nib/vendor"
.text_naranja
    color #f7a729 !important
    font-weight: bold;
body
    font-family: "amsi_proregular"
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    position: absolute;
    width: 100%;
    .wrapper
        width: 600px;
        height: 600px;
        overflow: hidden;
        box-shadow: 1px 3px 11px #ccc;
        .coffe-container 
            display: flex;
            transform: translateX(-0px);
            transition: all .3s ease-in-out
            .slide
                min-width: 600px;
                .logo
                    left: 2.5em;
                    position: relative;
                .section-Content
                    .col
                        width: 50% !important
                        .content
                            .brandsponsor
                                position: absolute;
                                bottom: -44px;
                                right: 0;
                                img
                                    height: 55px;
                            p
                                margin: .5em 1em;
                            .brand-cont
                                position: relative;
                                .brand
                                    height: 127px;
                                    margin: 0 auto;
                                    margin-top: 2em;
                                .mas
                                    height: 70px;
                                    position: absolute;
                                    bottom: 0;
                                    z-index: -1;
                        .go
                            img
                                position: absolute;
                                height: 33px;
                                right: 2em;
                                cursor pointer
                        .more
                            position: absolute;
                            right: -2em;
                            img
                                height: 70px;
                        .slide-control
                            display: flex;
                            justify-content: center
                            position: relative
                            z-index: 2
                            .active
                                border: 3px solid #f7a626;
                                div
                                span
                                    background: #f7a729 !important
                                    color: white !important
                            .control
                                width: 70px;
                                height: 70px;
                                display: flex;
                                align-items: center;
                                justify-content: center;
                                border-radius: 50%;
                                border: 3px solid #f7a626;
                                margin: .3em
                                transition: all .3s ease-in-out
                                cursor: pointer
                                &:hover
                                    div
                                        background: #f7a729 !important
                                        color: white !important 
                                div
                                span
                                    background: white;
                                    font-size: 1.8em;
                                    border-radius: 50%;
                                    color: #f7a729;
                                    width: 55px;
                                    height: 55px;
                                    text-align: center;
                                    line-height: 1.9;
                                    transition: all .3s ease-in-out
                                    
        .section
            &-Content
                width: 100%;
                display: flex;
                position: relative;
                top: -1.8em;
                .col
                    width: 33.33%;
                    position: relative
                    .back
                        width: 100%;
                        position: absolute;
                        top: 0;
                        z-index: -2
                        img
                            width: 100%
                        h6
                            margin: 0;
                            text-align: center;
                            color: #666666;
                            font-family:"grota_sans_rdbold"
                    .title 
                        margin-top: 2em;
                        img
                            height: 70px;
                            margin: 0 auto;
                    .coffe
                        height: 272px;
                        position: relative;
                        cursor: pointer
                        &:hover 
                            .go
                                margin-top: 1em
                            .glass
                                transform: scale(1.1)
                        .glass
                            position: absolute;
                            top: -5em;
                            z-index: -1;
                            transition: all .3s ease-in-out
                            img
                                width: 100%;
                        .go
                            position: absolute;
                            right: 0;
                            margin-top: 2em;
                            margin-right: .5em;
                            z-index: -2;
                            cursor: pointer
                            transition: all .3s ease-in-out
                            img
                                height: 78px;
                    
            &-Header
                position: relative;
                z-index: 2
                .back
                    position: absolute;
                    z-index: -1;
                    left: 0;
                    img
                        width: 100%;
                &-Main
                    display: flex;
                    justify-content: space-around;
                    align-items: center;
                    height: 205px;
                    .title
                        margin-top: -1em;
                        color: white;
                        h3
                            font-family:"grota_sans_rdlight"
                            margin: 0
                            b
                                font-family:"grota_sans_rdbold"
                        img
                            height: 113px;
                    .logo
                        img
                            height: 138px;
                            


    



